#include<bits/stdc++.h>
using namespace std;

void movzero(int arr[],int n)
{
    int count=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]!=0)
        {
            swap(arr[i],arr[count]);
            count++;
        }
    }

}
int main()
{
    int arr[]={10,20,0,30,0,0,0};
    int n=sizeof(arr)/sizeof(arr[0]);
    movzero(arr,n);
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
