#include<bits/stdc++.h>
using namespace std;

void NGR(int arr[],int n)
{
    vector<int>v;
    stack<int>s;
    for(int i=0;i<s.size();i--)
    {
        if(s.size()==0)
        {
            v.push_back(-1);
            s.push(arr[i]);
        }
        else if(s.size()>0 && s.top()>arr[i])
        {
            v.push_back(s.top());
            s.push(arr[i]);
        
        }
        else if(s.size()>0 && s.top()<=arr[i])
        {
            while(s.size()>0 && s.top()<=arr[i])
            {
                s.pop();
                s.push(arr[i]);
            }
            if(s.size()==0)
            {
                v.push_back(-1);
                s.push(arr[i]);
                
            }
            else
            {
                v.push_back(s.top());
                s.push(arr[i]);
            }
        }   
        
    }
    for(int i=0;i<v.size();i++)
    {
        cout<<v[i]<<" ";
    }
}
int main()
{
    int arr[]={1,3,2,4};
    int n=4;
    NGR(arr,n);
}
